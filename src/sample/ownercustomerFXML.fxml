<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.OwnercustomerFXMLController">
   <children>
      <TableView fx:id="oc_table" layoutX="74.0" layoutY="54.0" prefHeight="178.0" prefWidth="453.0">
        <columns>
          <TableColumn fx:id="col_username" prefWidth="188.0" text="Username" />
          <TableColumn fx:id="col_password" prefWidth="158.0" text="Password" />
            <TableColumn fx:id="col_points" prefWidth="106.0" text="Points" />
        </columns>
      </TableView>
      <Button fx:id="addCustomer" layoutX="310.0" layoutY="291.0" mnemonicParsing="false" onAction="#buttonAdd" text="Add" />
      <Button fx:id="deleteCustomer" layoutX="385.0" layoutY="291.0" mnemonicParsing="false" onAction="#buttonDelete" text="Delete" />
      <Label layoutX="28.0" layoutY="272.0" text="Username" />
      <Label layoutX="30.0" layoutY="302.0" text="Password" />
      <TextField fx:id="textfield_username" layoutX="97.0" layoutY="268.0" />
      <TextField fx:id="textfield_password" layoutX="97.0" layoutY="298.0" />
      <Button fx:id="back" layoutX="475.0" layoutY="291.0" mnemonicParsing="false" onAction="#buttonBack" text="Back" />
      <Button fx:id="logout" layoutX="280.0" layoutY="361.0" mnemonicParsing="false" onAction="#buttonLogout" text="Logout" />
   </children>
</AnchorPane>

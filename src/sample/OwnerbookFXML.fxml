<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.OwnerbookFXMLController">
   <children>
      <TableView fx:id="ob_table" layoutX="110.0" layoutY="33.0" prefHeight="236.0" prefWidth="398.0">
        <columns>
          <TableColumn fx:id="col_bookname" prefWidth="218.0" text="Book Name" />
          <TableColumn fx:id="col_bookprice" prefWidth="179.0" text="Book Price" />
        </columns>
      </TableView>
      <Button fx:id="addBook" layoutX="334.0" layoutY="309.0" mnemonicParsing="false" onAction="#buttonAddbook" text="Add" />
      <TextField fx:id="textfield_bookname" layoutX="110.0" layoutY="284.0" />
      <TextField fx:id="textfield_bookprice" layoutX="110.0" layoutY="321.0" />
      <Label layoutX="45.0" layoutY="288.0" text="Name" />
      <Label layoutX="45.0" layoutY="325.0" text="Price" />
      <Button fx:id="deleteBook" layoutX="411.0" layoutY="309.0" mnemonicParsing="false" onAction="#buttonDeletebook" text="Delete" />
      <Button fx:id="logout" layoutX="334.0" layoutY="361.0" mnemonicParsing="false" onAction="#buttonLogout" text="Log out" />
      <Button fx:id="back" layoutX="488.0" layoutY="309.0" mnemonicParsing="false" onAction="#buttonBack" text="Back" />
   </children>
</AnchorPane>
